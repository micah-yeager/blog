{
  "headers": [
    {
      "source": "/favicon.(.*)",
      "headers": [
        {
          "key": "Cloudflare-CDN-Cache-Control",
          "value": "max-age=86400, stale-while-revalidate=7776000"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    },
    {
      "source": "/build/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cloudflare-CDN-Cache-Control",
          "value": "max-age=1800, stale-while-revalidate=7776000"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
}
