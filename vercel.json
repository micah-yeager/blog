{
  "headers": [
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=1800, s-maxage=3600, stale-while-revalidate=900"
        }
      ]
    },
    {
      "source": "/build/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/fonts/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=1800, s-maxage=3600, stale-while-revalidate=900"
        }
      ]
    },
    {
      "source": "/images/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=1800, s-maxage=3600, stale-while-revalidate=900"
        }
      ]
    }
  ]
}
